datacenter = "lc"
domain = "<%= node['consul']['domain'] %>"
data_dir = "<%= node['consul']['data_dir'] %>"
retry_join = <%= @masters %>
enable_local_script_checks = true
performance = {
    raft_multiplier = 1
}

verify_incoming = true
verify_outgoing = true
verify_server_hostname = false
auto_encrypt = {
   allow_tls = false
}
ports = {
   http = <%= node['consul']['http_api_port'] %>
}